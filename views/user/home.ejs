        <div class="heading_container heading_center">
            <h2>New Arrivals</h2>
        </div>
        <div class="owl-carousel product_owl-carousel">
            <% for(let i=0; i < Math.ceil(products.length / 3); i++) { %>
                <div class="item">
                    <div class="row">
                        <% for(let j=i * 3; j < Math.min((i + 1) * 3, products.length); j++) { %>
                            <% if(products[j].isListed) { %>
                                <div class="col-sm-6 col-lg-4">
                                    <div class="box">
                                        <div>
                                            <div class="img-box position-relative">
                                                <a href="/user/menu" class="text-decoration-none">
                                                    <img src="/uploads/<%= products[j].image[0] %>" alt="<%= products[j].name %>">
                                                </a>
                                                <% if (products[j].maxOffer && products[j].maxOffer > 0) { %>
                                                    <div class="offer-badge">
                                                        <% if (products[j].maxOffer >= 50) { %>
                                                            <span class="big-offer">MEGA OFFER</span>
                                                        <% } %>
                                                        <% if (products[j].offerType === 'category') { %>
                                                            <span class="offer-type">Category Offer</span>
                                                        <% } %>
                                                        <%= products[j].maxOffer %>% OFF
                                                    </div>
                                                <% } %>
                                            </div>
                                            <div class="detail-box">
                                                <h5><%= products[j].name %></h5>
                                                <div class="options">
                                                    <div class="price-container">
                                                        <% if (products[j].maxOffer && products[j].maxOffer > 0) { %>
                                                            <span class="old-price">MRP: <del>₹<%= products[j].originalPrice %></del></span>
                                                            <span class="price">Offer: ₹<%= products[j].discountedPrice %></span>
                                                        <% } else { %>
                                                            <h6>MRP ₹<%= products[j].originalPrice %></h6>
                                                        <% } %>
                                                    </div>
                                                    <% if (user) { %>
                                                        <a onclick="addToCart('<%= products[j]._id %>', 1, '<%= products[j].name %>', '<%= user._id %>')" 
                                                           class="add-to-cart">
                                                    <% } else { %>
                                                        <a onclick="Swal.fire('Please log in to add items to the cart.')" 
                                                           class="add-to-cart">
                                                    <% } %>
                                                            <svg version="1.1" id="Capa_1" xmlns="http://www.w3.org/2000/svg"
